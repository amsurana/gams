feature (ros) {
  requires += ros

  macros += _GAMS_ROS_

  includes += $(ROS_ROOT)/../../include
  includes += /usr/include/libcxxabi
  includes += /usr/include/pcl-1.7/
  libpaths += $(ROS_ROOT)/../../lib
  libpaths += /usr/lib/x86_64-linux-gnu
  libs += rosconsole actionlib rosbag_storage rosbag rostime roscpp_serialization cpp_common roscpp roslib ros_type_introspection topic_tools tf2_ros pcl_common
}
